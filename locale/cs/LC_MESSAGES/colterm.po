#: colterm.py:625
msgid "finished in"
msgstr "dokončeno za"

#: colterm.py:831
msgid "Please, input a value from {0}."
msgstr "Zadejte prosím jednu z hodnot {0}."

#: colterm.py:835
msgid "y"
msgstr "a"

#: colterm.py:835
msgid "yes"
msgstr "ano"

#: colterm.py:837
msgid "n"
msgstr "n"

#: colterm.py:837
msgid "no"
msgstr "ne"

#: colterm.py:840
msgid "Please, answer yes/no (y/n)."
msgstr "Odpovězte prosím ano/ne (a/n)."

#: colterm.py:845
msgid "Use only digits, please."
msgstr "Prosím, použijte jen číslice."

#: colterm.py:850
msgid "Please, input a decimal number."
msgstr "Zadejte prosím desetinné číslo (s desetinnou tečkou)."

#: colterm.py:853
msgid "Please, use only alpha-numeric characters."
msgstr "Použijte prosím jen alfanumerické znaky."

#: colterm.py:856
msgid "Please, input a non-empty string."
msgstr "Zadejte prosím neprázdný řetězec."

#: colterm.py:858
msgid "ERROR"
msgstr "CHYBA"

#: colterm.py:869
msgid "Please, chose an option from the list - input a number, or a full text of the desired option."
msgstr "Vyberte prosím jednu z nabízených možností - zadejte buď číslo, nebo plný text požadované volby."

#: colterm.py:897
msgid "Select"
msgstr "Volba"

